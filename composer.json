{
    "name": "brandembassy/php-memory",
    "description": "A simple tool to provide an easy-to-use API to get memory configuration from php.ini.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Kvasnica",
            "email": "ivan@ikvasnica.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BrandEmbassy\\Memory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrandEmbassyTest\\Memory\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "marc-mabe/php-enum": "^3.0 || ^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "suggest": {
        "nette/di": "Needed for registering package using the Nette Extension."
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit tests",
        "phpstan": "vendor/bin/phpstan analyse src tests -l max -c phpstan.neon"
    }
}
